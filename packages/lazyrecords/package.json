    "bun-types": "1.2.3"
  },
  "exports": {
    "./Keyword.ts": "./src/Keyword.ts",
    "./sql/ColumnTypeMappings.ts": "./src/sql/ColumnTypeMappings.ts",
    "./sql/duckdb/duckdbMappings.ts": "./src/sql/duckdb/duckdbMappings.ts",
    "./sql/duckdb/DuckDBRecords.ts": "./src/sql/duckdb/DuckDBRecords.ts",
    "./sql/postgres/postgresMappings.ts": "./src/sql/postgres/postgresMappings.ts",
